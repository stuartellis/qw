{
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "package": "./package.json",
        "readme": "./README.md"
    }
}