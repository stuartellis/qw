{
  "name": "qw",
  "version": "0.4.0",
  "description": "Tooling for operations",
  "main": "bin/qw.js",
  "bin": {
    "qw": "./bin/qw.js"
  },
  "scripts": {
    "clean": "rimraf .nyc_output coverage docs test-results.xml tmp",
    "doc": "jsdoc -c jsdoc.json",
    "prepare": "husky install",
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "mocha 'test/**/*.test.js' --timeout 3000",
    "test:ci": "nyc --reporter=cobertura mocha 'test/**/*.test.js' --timeout 3000 --reporter mocha-junit-reporter"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stuartellis/qw.git"
  },
  "author": "Stuart Ellis <stuart@stuartellis.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stuartellis/qw/issues"
  },
  "homepage": "https://github.com/stuartellis/qw#readme",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.43.0",
    "@aws-sdk/client-s3": "^3.44.0",
    "@aws-sdk/signature-v4-crt": "^3.41.0",
    "axios": "^0.22.0",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "flat": "^5.0.2",
    "handlebars": "^4.7.7",
    "nodemailer": "^6.7.2",
    "simple-git": "^2.48.0",
    "urijs": "^1.19.7"
  },
  "devDependencies": {
    "aws-sdk-client-mock": "^0.5.6",
    "eslint": "^8.4.1",
    "husky": "^7.0.4",
    "jsdoc": "^3.6.7",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "nock": "^13.2.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2"
  }
}
